[Eth]
NetworkId = <%= config.geth.network.id %>
SyncMode = "full"
LightPeers = 100
DatabaseCache = 768
EnablePreimageRecording = false

[Eth.TxPool]
Locals = []
NoLocals = false
Journal = "transactions.rlp"
Rejournal = 3600000000000
PriceLimit = 1
PriceBump = 10
AccountSlots = 16
GlobalSlots = 4096
AccountQueue = 64
GlobalQueue = 1024
Lifetime = 10800000000000

[Eth.GPO]
Blocks = 20
Percentile = 60

[Node]
UserIdent = "<%= Node_UserIdent %>"
DataDir = "<%= Node_DataDir %>"
IPCPath = "<%= Node_DataDir %>/geth.ipc"
HTTPHost = "0.0.0.0"
HTTPPort = <%= Node_HTTPPort %>
HTTPCors = ["*"]
HTTPVirtualHosts = ["*"]
HTTPModules = ["net", "web3", "eth"]
WSHost = "0.0.0.0"
WSPort = <%= Node_WSPort %>
WSOrigins = ["*"]
WSModules = ["net", "web3", "eth"]

[Node.P2P]
MaxPeers = 50
NoDiscovery = false
BootstrapNodes = []
BootstrapNodesV5 = []
StaticNodes = []
TrustedNodes = []
ListenAddr = ":<%= NodeP2P_DiscoveryAddr %>"
EnableMsgEvents = false